{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020", "DOM"],
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": {
      "@animts/*": ["docs/anims/_lib/*"]
    },
    "strict": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "rootDir": "docs/anims",
    "outDir": "assets/anims",
    "sourceMap": true
  },
  "include": ["docs/anims/**/*.ts"]
}
